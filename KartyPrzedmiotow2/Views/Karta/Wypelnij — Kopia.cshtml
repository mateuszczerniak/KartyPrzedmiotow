<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="~/Scripts/dynamicTable.js"></script>
@model KartyPrzedmiotow2.Models.Karta
@{
    ViewBag.Title = "Karta";
}

<style>
    .slider {
        transition-property: all;
        transition-duration: 1s;
        transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
    }
    .slider.closed {
        display: none;
    }
    .full-width {
        width: 100%;
    }
    .table-name{
        display: none;
    }
    .hide{
        display: none;
    }
</style>

<div class="jumbotron">
    <h1>Karty Przedmiotów</h1>
    <p class="lead">Dodawanie nowej karty dla przedmiotu.</p>
</div>

<div class="row align-items-center">

    @using (Html.BeginForm("Wypelnij", "Karta", FormMethod.Post, new { @class = "form-horizontal" }))
    {
        @Html.HiddenFor(m => m.ProgramId)

    
    <h1 id="opis" class="sectionHeader">Opis ↓</h1>

        <div id="opisDetails" class="col-md-12 slider closed">
            <h2>Informacje</h2>
            <div class="form-group row">
                <label for="nazwaAng" class="col-md-4 col-form-label">Nazwa w języku angielskim</label>
                <div class="col-md-6">
                    @Html.TextBoxFor(model => model.NazwaAngielski, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group row">
                <label for="jezyk" class="col-md-4 col-form-label">Język prowadzenia zajęć</label>
                <div class="col-md-6">
                    @Html.DropDownListFor(m => m.JezykiId, Model.Jezyki, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group row">
                <label for="materialy" class="col-md-4 col-form-label">Materiały dla studentów</label>
                <div class="col-md-6">
                    @Html.DropDownListFor(m => m.MaterialyId, Model.Materialy, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group row">
                <label for="wykladowca" class="col-md-4 col-form-label">Wykładowca</label>
                <div class="col-md-6">
                    @Html.DisplayFor(m => m.Imie) @Html.DisplayFor(m => m.Nazwisko)
                </div>
            </div>
            <div class="form-group row">
                <label for="rok" class="col-md-4 col-form-label">Rok akademicki</label>
                <div class="col-md-6">
                    @Html.TextBoxFor(m => m.Rok, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group row">
                <label for="opis" class="col-md-4 col-form-label">Krótki opis</label>
                <div class="col-md-6">
                    @Html.TextAreaFor(m => m.KrotkiOpis, new { @class = "form-control" })
                </div>
            </div>
            <h2>Cele dydaktyczne</h2>
