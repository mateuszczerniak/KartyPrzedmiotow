                                        <tr>
                                            <td>
                                                MP@(i + 1)
                                            </td>
                                            <td>
                                                @Html.DropDownListFor(m => Model.MetodyPodajaceIds[i], Model.MetodyPodajace.Select(mp => new SelectListItem()
                                           {
                                               Text = mp.Text,
                                               Value = mp.Value,
                                               Selected = mp.Value == Model.MetodyPodajaceIds[i].ToString()
