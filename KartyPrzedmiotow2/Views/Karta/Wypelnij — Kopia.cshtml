            <div class="container" id="tabela1">
                <div class="row clearfix">
                    <div class="col-md-12 column">
                        <div class="table-name" id="@Html.IdFor(model => model.LiteraturyPodstawowe)" name="@Html.NameFor(model => model.LiteraturyPodstawowe)"></div>
                        <table class="table table-bordered table-hover" id="tab_logic">
                            <thead>
                                <tr>
                                    <th class="text-center">
                                        Tytuł
                                    </th>
                                </tr>
                            </thead>
                            <tbody class="list-rows">

                                @for (var i = 0; i < Model.LiteraturyPodstawowe.Count; i++)
                                {
                                    <tr>
                                        <td>
                                            @Html.TextBoxFor(model => model.LiteraturyPodstawowe[i].Nazwa, new { @class = "form-control input-md full-width"})
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
                <a class="add_row btn btn-default pull-left">Dodaj</a>
                <a class="delete_row pull-right btn btn-default">Usuń</a>
            </div>




